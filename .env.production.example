# Production Environment Variables
# Copy this to .env.production and fill in your values

# MongoDB
MONGO_PASSWORD=your-strong-mongodb-password-here

# Backend Secrets (generate with: openssl rand -hex 32)
JWT_SECRET=your-super-secret-jwt-key-64-characters-minimum-here
REFRESH_TOKEN_SECRET=your-another-secret-refresh-token-64-chars-here

# URLs (update with your domain)
FRONTEND_URL=https://yourdomain.com
API_URL=https://api.yourdomain.com/api

# Optional: Google Integration
GOOGLE_PLACES_API_KEY=
GOOGLE_PLACE_ID=
